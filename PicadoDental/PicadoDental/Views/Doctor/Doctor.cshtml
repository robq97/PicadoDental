@model IEnumerable<PicadoDental.Models.MDoctor>

@if (Session["TipoUsuario"] != null && Session["TipoUsuario"].ToString().Equals("Secretaria") | Session["TipoUsuario"].ToString().Equals("Doctor"))
{
    {
        ViewBag.Title = "Doctor/{id}";
    }
    if (TempData["message"] != null)
    {
        <body onload="alerta()">
        </body>
    }
    foreach (var p in Model)
    {
        <div class="container body-content">
            <div class="row" style="margin-top: 5%">
                <div class="card green darken-3 white-text">
                    <div class="center">
                        <span class="card-title" style="font-size: 55px">Información del Doctor</span>
                    </div>
                    <ul class="collection with-header black-text">
                        <li class="collection-header"><h4>Cedula:</h4></li>
                        <li class="collection-item">@p.Cedula</li>
                        <li class="collection-header"><h4>Nombre:</h4></li>
                        <li class="collection-item">@p.Nombre</li>
                        <li class="collection-header"><h4>Apellidos:</h4></li>
                        <li class="collection-item">@p.Apellidos</li>
                        <li class="collection-header"><h4>Telefono:</h4></li>
                        <li class="collection-item">@p.Telefono</li>
                        <li class="collection-header"><h4>Correo:</h4></li>
                        <li class="collection-item">@p.Correo</li>
                        <li class="collection-header"><h4>Genero:</h4></li>
                        <li class="collection-item">@p.Genero</li>
                    </ul>
                </div>
            </div>
        </div>
    }
}

@if (Session["TipoUsuario"] == null || Session["TipoUsuario"] != null && Session["TipoUsuario"].ToString().Equals("Admin"))
{
    Response.Redirect("~/Home/Index");
}
